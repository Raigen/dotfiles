alias ~='cd ~'
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'

alias reload='source ~/.bashrc'

alias ls='ls -1 --color'
alias l='ls -lah'
alias gvimdiff='gvim -d'
alias vimmdiff='vim -d'

alias npme='npm --registry http://registry.npmjs.eu'

alias gitclean='git o develop && git branch --merged | grep feature | xargs git branch -d'
